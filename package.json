  "description": "Template Backend",
  "main": "server.js",
  "scripts": {
    "build": "babel src --out-dir build --extensions '.ts' --copy-files",
    "tsc": "tsc",
    "test": "jest --detectOpenHandles",
    "dev": "pm2-dev start pm2/dev.json",
    "dev:script": "NODE_ENV=development babel-node --extensions \".ts\" src/scripts/index.ts",
    "dev:migrate": "npm run dev:script migrate",
    "dev:seeds": "npm run dev:script seeds",
    "dev:types": "npm run dev:script generate-types",
    "start": "pm2-runtime start pm2/prod.json",
    "prestart": "npm run migrate && npm run seeds",
    "prod:script": "NODE_ENV=production node build/scripts/index.js",
    "migrate": "npm run prod:script migrate",
    "seeds": "npm run prod:script seeds",
    "lint:fix": "npm run lint --fix",
    "lint": "eslint './src/**/*.ts'"
  },
